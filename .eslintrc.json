{
  "extends": ["airbnb-base","airbnb"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "space-before-function-paren": ["warn", "never"],
    "arrow-body-style": ["error", "as-needed"],
    "func-names": ["error", "never"]

  },
  "env": {
    "browser": true,
    "node": true,
    "webextensions":true
  }
}
